<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <managed-bean>
        <managed-bean-name>SessionBean</managed-bean-name>
        <managed-bean-class>gnomeshop.SessionBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ApplicationBean</managed-bean-name>
        <managed-bean-class>gnomeshop.ApplicationBean</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RequestBean</managed-bean-name>
        <managed-bean-class>gnomeshop.RequestBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>LoginManager</managed-bean-name>
        <managed-bean-class>gnomeshop.LoginManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ProductBean</managed-bean-name>
        <managed-bean-class>gnomeshop.items.ProductBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ShoppingCartBean</managed-bean-name>
        <managed-bean-class>gnomeshop.items.ShoppingCartBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>OrderBean</managed-bean-name>
        <managed-bean-class>gnomeshop.items.OrderBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>MemberBean</managed-bean-name>
        <managed-bean-class>gnomeshop.items.MemberBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Register</managed-bean-name>
        <managed-bean-class>gnomeshop.Register</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Login</managed-bean-name>
        <managed-bean-class>gnomeshop.Login</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Search</managed-bean-name>
        <managed-bean-class>gnomeshop.Search</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Header</managed-bean-name>
        <managed-bean-class>gnomeshop.Header</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Footer</managed-bean-name>
        <managed-bean-class>gnomeshop.Footer</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>index</managed-bean-name>
        <managed-bean-class>gnomeshop.index</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>AdminPage</managed-bean-name>
        <managed-bean-class>gnomeshop.AdminPage</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Manage</managed-bean-name>
        <managed-bean-class>gnomeshop.Manage</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ShoppingCart</managed-bean-name>
        <managed-bean-class>gnomeshop.ShoppingCart</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>MemberDetail</managed-bean-name>
        <managed-bean-class>gnomeshop.MemberDetail</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Checkout</managed-bean-name>
        <managed-bean-class>gnomeshop.Checkout</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>GnomeDetail</managed-bean-name>
        <managed-bean-class>gnomeshop.GnomeDetail</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Inventory</managed-bean-name>
        <managed-bean-class>gnomeshop.Inventory</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/ShoppingCart.jsp</from-view-id>
        <navigation-case>
            <from-outcome>Checkout</from-outcome>
            <to-view-id>/Checkout.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>Login</from-outcome>
            <to-view-id>/Login.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/AdminPage.jsp</from-view-id>
        <navigation-case>
            <from-outcome>MemberDetail</from-outcome>
            <to-view-id>/MemberDetail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>Order</managed-bean-name>
        <managed-bean-class>gnomeshop.Order</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ProductDetail</managed-bean-name>
        <managed-bean-class>gnomeshop.ProductDetail</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>AddingIntoCartSuccess</managed-bean-name>
        <managed-bean-class>gnomeshop.AddingIntoCartSuccess</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>AddingIntoCartFailure</managed-bean-name>
        <managed-bean-class>gnomeshop.AddingIntoCartFailure</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/GnomeDetail.jsp</from-view-id>
        <navigation-case>
            <from-outcome>AddingSuccess</from-outcome>
            <to-view-id>/AddingIntoCartSuccess.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>AddingFailure</from-outcome>
            <to-view-id>/AddingIntoCartFailure.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Checkout.jsp</from-view-id>
        <navigation-case>
            <from-outcome>submit</from-outcome>
            <to-view-id>/Order.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>SubmissionFail</managed-bean-name>
        <managed-bean-class>gnomeshop.SubmissionFail</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/Search.jspf</from-view-id>
        <navigation-case>
            <from-outcome>search</from-outcome>
            <to-view-id>/Inventory.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>SearchBean</managed-bean-name>
        <managed-bean-class>gnomeshop.SearchBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RegisterFailed</managed-bean-name>
        <managed-bean-class>gnomeshop.RegisterFailed</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Loginsuceed</managed-bean-name>
        <managed-bean-class>gnomeshop.Loginsuceed</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Loginerror</managed-bean-name>
        <managed-bean-class>gnomeshop.Loginerror</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RegisterSuceed</managed-bean-name>
        <managed-bean-class>gnomeshop.RegisterSuceed</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>
